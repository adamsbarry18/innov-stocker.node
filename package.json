{
  "name": "innov-stocker.node",
  "version": "0.0.1",
  "type": "module",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "build": "npm run lint && tsc && tsc-alias",
    "prod": "cross-env NODE_ENV=production node dist/server.js",
    "lint": "rimraf dist && eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "clean": "rimraf dist && mkdir dist",
    "test:coverage": "cross-env NODE_ENV=test dotenv -e .env.test vitest run --coverage --color",
    "test:local": "cross-env NODE_ENV=development dotenv -e .env.local vitest run --color",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test vitest run --color",
    "test-db:start": "docker compose -f src/tests/docker-compose.yml up --build -d",
    "test-db:stop": "docker compose -f src/tests/docker-compose.yml down -v",
    "test:all": "bash ./src/tests/test-with-clean-db.sh",
    "prune": "docker system prune -af",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "glob": "^11.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "qs": "^6.14.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamsbarry18/innov-stocker.node.git"
  }
}