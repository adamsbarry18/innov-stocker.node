{
    "version": 2,
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install"
}