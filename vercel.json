{
    "version": 2,
    "builds": [
        {
            "src": "api/index.mjs",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "api/dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.mjs"
        }
    ],
    "buildCommand": "npm run build:vercel"
}