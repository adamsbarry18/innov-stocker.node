{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build:vercel"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "functions": {
        "src/server.ts": {
            "maxDuration": 30
        }
    }
}